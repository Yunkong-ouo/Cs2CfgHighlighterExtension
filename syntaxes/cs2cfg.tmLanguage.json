    "sourcemod": {
      "patterns": [
        {
          "name": "keyword.operator.quantifier.regexp",
          "match": "(?i)\\b(cl_radial_radio_tab|cl_radial_radio_tab_[0-2]_text_[1-8]|clear|clearall|cvarlist|cyclevar|echo|echoln|exec|exec_async|exec_async_wait|execifexists|grep|host_writeconfig|incrementvar|log_(color|flags)|menuselect|messagemode|messagemode2|multvar|quit|reset_gameconvars|restart_(in_(insecure|trusted|untrusted)|normal)|say|say_team|setinfo|sleep|toggle|unbindall|writekeybindings)\\b"
        }
      ]
