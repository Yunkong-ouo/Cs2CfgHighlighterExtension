    "exec": {
      "patterns": [
        {
          "name": "constant.language",
          "match": "(?i)(?<!//)(?<=\\b(exec|exec_async|exec_async_wait|execifexists)\\s+)(\"?)([\\w\\p{L}\\$%+-.~*/\\\\]+)(\"?)(?=\\s*(?!//))",
          "captures": {
            "2": { "name": "string.quoted.double" },
            "3": { "name": "constant.language" },
            "4": { "name": "string.quoted.double" }
          }
        }
      ]
    },
    "CS-command": {
      "patterns": [
        {
          "name": "keyword.control",
